{
  "name": "vuelscroller",
  "private": false,
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vuelcomponents/vuelscroller.git"
  },
  "homepage": "https://vuelcomponents.github.io",
  "keywords": [
    "vuelscroller",
    "scroller",
    "virtual",
    "virtual scroller",
    "api",
    "backend",
    "request rendering",
    "api scrolling",
    "pagination",
    "vuel"
  ],
  "files": [
    "dist"
  ],
  "rules": {
    "no-unused-vars": "off"
  },
  "main": "./dist/vuelscroller.cjs",
  "module": "./dist/vuelscroller.js",
  "exports": {
    ".": {
      "import": "./dist/vuelscroller.js",
      "require": "./dist/vuelscroller.cjs"
    }
  },
  "dependencies": {
    "vite-plugin-dts": "^3.7.3",
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "autoprefixer": "^10.4.17",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.22"
  }
}
